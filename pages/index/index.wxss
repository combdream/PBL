/* 顶部 */.top{
    width: 100vw;
    height: 80rpx;
    padding: 40rpx 0rpx 20rpx 155rpx;
    line-height: 80rpx;
    font-size: 40rpx;
    border-bottom: 1px solid rgb(253, 249, 249);
  }.top-text{
    float: left}/* 下拉框 */.top-selected{
      width: 50%;
      display: flex;
      float: right;
      align-items: center;
      justify-content: space-between;
      border: 1px solid rgb(248, 246, 246);
      margin: 0rpx 50rpx 0rpx 150rpx;
      font-size: 40rpx;
  }/* 下拉内容 */.select_box {
      background-color: rgb(250, 246, 246);
      padding: 50rpx 50rpx;
      margin: 20rpx 0rpx;
      width: 50%;
      float: right;
      position: absolute;
      right: 0;
      z-index: 1;
      overflow: hidden;
      text-align: left;
      animation: myfirst 0.5s;
      font-size: 30rpx;
  }.select_one {
      padding-left: 20rpx;
      width: 100%;
      height: 60rpx;
      position: relative;
      line-height: 60rpx;
      border: 1px 1px solid #ccc;
  }/* 下拉过度效果 */
  @keyframes myfirst {
      from {
          height: 0rpx;
      }
      to {
          height: 210rpx;
      }}/* 下拉图标 */
    .top-selected image{
    height:50rpx;
    width:50rpx;
    position: relative;
    right: 0rpx;
    top: 10rpx;
  }
